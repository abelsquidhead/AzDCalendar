{
  "manifestVersion": 1,
  "id": "WorkItemCalendar",
  "publisher": "abelwang",
  "version": "1.0.201",
  "name": "Work Item Calendar",
  "description": "A simple calendar that displays work items in a calendar based off of a user controlled date field",
  "public": false,
  "categories": ["Azure Boards"],
  "targets": [
      {
          "id": "Microsoft.VisualStudio.Services"
      }
  ],
  "scopes": ["vso.work"],
  "icons": {
    "default": "static/images/squid-head-icon.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/Microsoft/vsts-team-calendar/issues"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vsts-team-calendar"
  },
  "contributions": [
      {
          "id": "WorkItemCalendar",
          "type": "ms.vss-web.hub",
          "targets": [
              "ms.vss-work-web.work-hub-group"
          ],
          "properties": {
              "name": "Calendar",
              "uri": "cal.html"
          }
      }
  ],
  "files": [
      {
          "path": "cal.html",
          "addressable": true
      },
      {
        "path": "static",
        "addressable": true
      },
      {
          "path": "node_modules/vss-web-extension-sdk/lib",
          "addressable": true,
          "packagePath": "lib"
      },
      {
        "path": "node_modules/fullcalendar",
        "addressable": true,
        "packagePath": "lib/fullcalendar"
    }
  ]
}